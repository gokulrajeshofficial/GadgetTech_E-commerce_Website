<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    function cancelOrder(orderId, proId) {
        Swal.fire({
            title: 'Are you sure?',
            text: "You are about to cancel this product!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, cancel it!'
        }).then((result) => {
            if (result.isConfirmed) {
                /*.................................................................AJAX..................................................*/
                $.ajax({
                    url: '/order/cancel',
                    method: "get",
                    data: { orderId, proId },
                    success: (order) => {
                        Swal.fire(
                            'Cancelled!',
                            'The Order has been cancelled .',
                            'success',
                        ),

                            document.getElementById('orderStatus').innerText = "Cancelled";
                        /* document.getElementById("cancel" + orderId).style.display = "none"*/


                    }
                })
                /*.................................................................AJAX..................................................*/
            }
        })

    }
</script>

<script>
    function returnOrder(orderId, proId) {
        $('#return-modal').modal('show')

        $('#returnForm').submit((e) => {
            e.preventDefault()
            let msg = document.getElementById('returnMsg').value
            console.log(msg)
            $.ajax({
                url: '/orders/returnOrder',
                method: 'post',
                data: { orderId, proId, msg },
                success: (response) => {
                    $('#return-modal').modal('hide')
                    console.log(response)
                    Swal.fire({
                        icon: 'success',
                        title: 'Request has been intiated',
                        text: ' The return will be initated when the seller approves the request',
                    })

                    document.getElementById('orderStatus').value = response

                }
            })

        })
    }
</script>